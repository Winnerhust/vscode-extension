{
    "name": "lookupreferences",
    "displayName": "lookupReferences",
    "description": "look up all references of a symbol,and build links to access them easily.",
    "version": "0.0.1",
    "publisher": "WinnerHust",
    "engines": {
        "vscode": "^1.25.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Winnerhust/vscode-extension/tree/master/lookupreferences"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:editor.printReferences"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "editor.printReferences",
            "title": "Show All References"
        }, {
            "command": "references.preLink",
            "title": "Go to pre referece"
        }, {
            "command": "references.nextLink",
            "title": "Go to next referece"
        }],
        "menus": {
            "editor/context": [{
                "command": "editor.printReferences",
                "when": "editorHasReferenceProvider",
                "group": "navigation@1.31"
            }],
            "editor/title": [{
                "command": "references.preLink",
                "group": "navigation"
            }, {
                "command": "references.nextLink",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}